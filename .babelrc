{
  "presets": [
    ["@babel/preset-env", {
      "targets": {
        "chrome": "67"
      },
      "corejs": "3",
      "useBuiltIns": "usage"
    }],
    ["@babel/preset-react"]
  ],
  "plugins": [
    ["@babel/plugin-transform-runtime", {
      "corejs": 2,
      "helpers": true,
      "regenerator": true,
      "useESModules": false
    }],
    ["syntax-dynamic-import"],
  ]
}